            </statement>
            <solution>
                <p>
                    <ol marker="a)" cols="2">
                        <li>
                            <p>
                                <md>
                                     <mrow>15^2 \amp = 15 \times 15 </mrow>
                                     <mrow> \amp = 225 </mrow>
                                </md>
                            </p>
